// Draw text
draw_set_font( ftGUI );
draw_text_shadow( 10, 10, "Advanced 3D texture filtering by MantaGames" );

draw_set_font( ftGUI_small );
// Filter type info
var _filterMode = "NONE";
switch( cFilterMethod ){
    case TextureFiltering.NONE:             _filterMode = "NONE"; break;
    case TextureFiltering.LINEAR:           _filterMode = "LINEAR"; break;
    case TextureFiltering.NEAREST_POINT:    _filterMode = "NEAREST POINT"; break;
    case TextureFiltering.BILINEAR:         _filterMode = "BILINEAR (GUASSIAN QUAD)"; break;
    case TextureFiltering.ANISOTROPIC:      _filterMode = "ANISOTROPIC"; break;

}
draw_text_shadow_colour( 10, 40, "Filter Mode: ", c_white );
draw_text_shadow_colour( 10+string_width( "Filter Mode: " ), 40, string(_filterMode), uiColour );
draw_text_shadow_colour( 10 + string_width( "Filter Mode: BILINEAR (GUASSIAN QUAD)" ) + 16, 40, "( Press Space to change )", c_gray );
// Anisotropy info
if( cFilterMethod == TextureFiltering.ANISOTROPIC ){
    draw_text_shadow_colour( 10, 60, "    Anisotropic level: ", c_ltgray );
    draw_text_shadow_colour( 10+string_width("    Anisotropic level: "), 60, "x"+string(anisotropy), uiColour );
    draw_text_shadow_colour( 10 + string_width( "Filter Mode: BILINEAR (GUASSIAN QUAD)" ) + 16, 60, "( Press Enter to change )", c_gray );
}

// Draw fps
draw_text_shadow_colour( 10, 80, "FPS: ", c_white );
