// initialise texture filtering
/*
    This function sets up the extension. This does not actually enable texture filtering, however
    it is required before you can use the texture filtering functions.
*/
tf_init();

////////////////////////////////////////
// Setup camera
camera_init();

// Setup 3D
d3d_start();
d3d_set_hidden( true );

// Demo variables
cFilterMethod   = 0;
anisotropy      = 0;

//////////////////////////////////////
// Load map assets
map_Trees = d3d_model_create();     d3d_model_load( map_Trees, "bk_Tree.png.gmmod" );
map_Grass = d3d_model_create();     d3d_model_load( map_Grass, "GrassBlade.png.gmmod" );
