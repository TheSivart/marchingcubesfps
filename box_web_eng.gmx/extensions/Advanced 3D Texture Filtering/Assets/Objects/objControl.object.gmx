map_Cargo   = d3d_model_create();   d3d_model_load( map_Cargo, "CargoContainer_1_D2.png.gmmod" );
map_Boulder = d3d_model_create();   d3d_model_load( map_Boulder, "Boulder_D.png.gmmod" );
map_Concrete = d3d_model_create();  d3d_model_load( map_Concrete, "ch_concrete_05_col.png.gmmod" );
map_CWAll    = d3d_model_create();  d3d_model_load( map_CWAll , "ch_concretewall07_col.png.gmmod" );

// skybox
global.sky = d3d_model_create();
d3d_model_ellipsoid( global.sky, 768-4096*2, 768-4096*2, -2048*2, 768+4096*2, 768+4096*2, 1024*2, 1, 1, 20 );
