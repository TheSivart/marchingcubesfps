            <string>//controls

if (keyboard_check(vk_shift))
{
    if (MoveSpeed&lt;MoveMax)
        MoveSpeed+=.5;
    else
        MoveSpeed=MoveMax;
        
    if (!keyboard_check(ord('W'))&amp;&amp;!keyboard_check(ord('A'))&amp;&amp;!keyboard_check(ord('S'))&amp;&amp;!keyboard_check(ord('D')))
        MoveSpeed=2;
}
else
    MoveSpeed=2;

speed=0;
if (keyboard_check(ord('W')))
    speed=MoveSpeed;
if (keyboard_check(ord('S')))
    speed=-MoveSpeed;
if (keyboard_check(ord('A')))
{
    x+=lengthdir_x(MoveSpeed,direction+90);
    y+=lengthdir_y(MoveSpeed,direction+90);
}
if (keyboard_check(ord('D')))
{
    x+=lengthdir_x(MoveSpeed,direction-90);
    y+=lengthdir_y(MoveSpeed,direction-90);
}
</string>
